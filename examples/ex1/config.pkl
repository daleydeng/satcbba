data {
  satellites {
    mode = "random"
    random {
        count = 6
        speed = 28000
    }
  }

  tasks {
    mode = "random"
    random {
        count = 20
        score_range = List(10.0, 1000.0)
        duration_range = List(60.0, 600.0)
    }
  }
}

viz {
  enabled = true
  enable_map = true
  show_task_info = true
  show_path_time = true
  image_size = List(1920, 960)
  font_size = 24.0
  line_thickness = 4
}

algo {
  method = "cbba"
  cbba {
    max_tasks_per_agent = 0
  }
}

output_config {
  dir = "examples/ex1/results"
  use_timestamp = true
  timestamp_fmt = "%Y-%m-%d_%H-%M-%S"
}
